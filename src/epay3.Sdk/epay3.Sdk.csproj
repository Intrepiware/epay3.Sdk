<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <PackageId>epay3.Sdk.Experimental</PackageId>
    <Version>1.0.0</Version>
    <Authors>Mike Devlin</Authors>
    <Company>epay3, LLC</Company>
    <Description>Unofficial (Experimental) .NET SDK for the epay3 Payment API</Description>
    <PackageProjectUrl>https://github.com/epay3/epay3.Sdk</PackageProjectUrl>
    <RepositoryUrl>https://github.com/epay3/epay3.Sdk</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="epay3.Sdk.Integration.Tests" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>
</Project>